{
  "name": "wizmi_api",
  "version": "0.1.0",
  "description": "Wizmi API - Built in Fastify & Prisma",
  "keywords": [
    "learning",
    "wizmi",
    "fastify",
    "prisma"
  ],
  "homepage": "https://github.com/Miaouuu/wizmi_api",
  "private": "true",
  "engines": {
    "node": ">=16.13.1",
    "pnpm": ">=6.24.2"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./build && tsc --project tsconfig.build.json",
    "start": "pnpm run build && node build/index.js",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "test": "jest --coverage",
    "prisma:schema:_init": "cat ./prisma/database.prisma > ./prisma/schema.prisma",
    "prisma:schema:_merge": "cat ./prisma/models/*.prisma >> ./prisma/schema.prisma",
    "prisma:schema:merge": "pnpm run prisma:schema:_init && pnpm run prisma:schema:_merge && pnpm prisma format",
    "prisma:schema:format": "pnpm prisma format",
    "prisma:schema:generate": "pnpm run prisma:schema:merge && pnpm prisma generate",
    "prisma:migration:dev": "pnpm prisma migrate dev",
    "prisma:migration:prod": "pnpm prisma migrate deploy",
    "prisma:migration:status": "pnpm prisma migrate status",
    "prisma:migration:reset": "pnpm prisma migrate reset",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "(node -e \"process.env.NODE_ENV === 'production' && process.exit(1)\" && husky install) || echo \"$NODE_ENV\""
  },
  "author": "Ruben",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "tap": "^16.3.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "@linear/sdk": "^1.22.0",
    "@prisma/client": "^3.6.0",
    "@sentry/node": "^6.16.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.24.1",
    "fastify-auth": "^1.1.0",
    "fastify-cors": "^6.0.2",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.12.6",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2",
    "wizmi": "latest"
  }
}
